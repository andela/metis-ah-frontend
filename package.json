  "description": "A Social platform for the creative at heart",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "cypress:open": "concurrently --kill-others \"npm run dev\" \"cypress open\"",
    "cypress:onci": "webpack-dev-server",
    "heroku-postbuild": "rimraf dist && npm run build",
    "dev": "webpack-dev-server --watch --open",
    "start": "node --require @babel/register ./src/server/index.js",
    "test": "jest --env=jsdom --verbose --updateSnapshot",
    "test:watch": "jest --watchAll --verbose",
    "format": "prettier src/**/*.{js,jsx,css,html,json}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/metis-ah-frontend.git"
